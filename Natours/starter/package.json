{
  "name": "natour",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node_modules/node-sass/bin/node-sass sass/main.scss css/style.css -w",
    "live:server": "node_modules/live-server/live-server.js",
    "start": "node_modules/concurrently/bin/concurrently.js --kill-others \"npm run watch:sass\" \"npm run live:server\"",
    "compile:sass": "node_modules/node-sass/bin/node-sass sass/main.scss css/style.comp.css",
    "concat:css": "node_modules/concat/bin/concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "node_modules/postcss-cli/bin/postcss  --no-map --use autoprefixer -b 'last 5 versions' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node_modules/node-sass/bin/node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "node_modules/npm-run-all/bin/npm-run-all/index.js compile:sass compile:sass prefix:css compress:css"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "concat": "^1.0.3",
    "concurrently": "^4.1.0",
    "live-server": "^1.2.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2"
  }
}
